{
  "mcpServers": {
    "aws_docs": {
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "command": "uvx",
      "disabled": false,
      "env": {
        "AWS_DOCUMENTATION_PARTITION": "aws",
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${cmd: echo $CONTEXT7_API_KEY}"
      },
      "command": "npx"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "sqlite": {
      "args": [
        "mcp-server-sqlite",
        "--db-path",
        "${TMPDIR}mcp-server-sqlite.db"
      ],
      "command": "uvx"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true
    },
    "neovim": {
      "disabled": true
    }
  }
}
